#include <iostream>
#include "Quadruplet.h"

Quadruplet::Quadruplet(){
	std::cout << "In quad" << std::endl;
}

Quadruplet::Quadruplet(std::string label,int value,Type type,Kind kind): label(label),value(value),type(type), kind(kind){
	std::cout << "In quad" << std::endl;
}

void Quadruplet::display()const{
	std::cout <<label;	
}

std::string Quadruplet::getLabel()const{
	return label;
}

void Quadruplet::setLabel(std::string label){
	label = label;
}

Type Quadruplet::getType()const{
	return type;
}

void Quadruplet::setType(Type type){
	type = type;
}

Kind Quadruplet::getKind()const{
	return kind;
}

void Quadruplet::setKind(Kind kind){
	kind = kind;
}

int Quadruplet::getValue()const{
	return value;
}

void Quadruplet::setValue(int value){
	value = value;
}
