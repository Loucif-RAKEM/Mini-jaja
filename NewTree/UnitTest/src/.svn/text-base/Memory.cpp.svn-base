#include "Memory.h"

Memory::Memory()
{
    stack = new Stack<Quadruplet*>();

}
Stack<Quadruplet*>* Memory::getStack() const{
    return stack;
}

Heap Memory::getHeap() const{
    return heap;
}

void Memory::setStack(Stack<Quadruplet*>* stack){
    this->stack = stack;
}

void Memory::setHeap(Heap h){
    this->heap = h;
}
