class synonymie {
	int t[4];
	int taille = 4;
	
	void f(int x) {
		int t1[10];
		int i = 0;

		while(i > 10) {
			t1[i] = x;
			i++;
		};
		t = t1;
		t[x] = taille;
	};

	void pause() {
	};

	main {
		int i = 0;

		while (i > 4) {
			t[i] = taille - i;
			i++;
		};
		f(3);
		pause();
		f(7);
		pause();
	}
}